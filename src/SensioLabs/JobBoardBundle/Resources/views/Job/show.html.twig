{% extends "SensioLabsJobBoardBundle::layout.html.twig" %}
{% from 'SensioLabsJobBoardBundle:Includes:breadcrumb.html.twig' import breadcrumb %}

{% block page_class 'preview' %}
{% block title 'Developer @ SensioLabs' %}

{% block content %}
    {{ breadcrumb({
        (announcement.country|country_name):    path('homepage', {'announcement_filters[country]' : announcement.country}),
        (announcement.contractType): path('homepage', {'announcement_filters[contractType]' : announcement.contractType}),
        (announcement.title): null,
    }) }}

    {% embed 'SensioLabsJobBoardBundle:Includes:job.html.twig' %}{% endembed %}
    <div class="clear"></div>
{% endblock %}
